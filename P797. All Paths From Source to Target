# dfs from 0 to N - 1
# time O(2^n)
# space O(2^n)

class Solution(object):
    def allPathsSourceTarget(self, graph):
        
        self.res = []
        self.dfs(graph, 0, [0])
        return self.res
    
    def dfs(self, graph, index, path):
        if index == len(graph) - 1:
            self.res.append(path)
            return 
        
        for nb in graph[index]:
            self.dfs(graph, nb, path + [nb])
