# bfs
# time O(n)
# space O(n)
class Solution(object):
    def isCompleteTree(self, root):
        """
        :type root: TreeNode
        :rtype: bool
        """
        if not root:
            return True
        queue = collections.deque([(root, 1)])
        res = []
        while queue:
            node, coord = queue.popleft()
            res.append(coord)
            if node.left:
                queue.append((node.left, coord * 2))
            if node.right:
                queue.append((node.right, coord * 2 + 1))
        return res[-1] == len(res)
