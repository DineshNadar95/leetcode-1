class Solution(object):
    def findMin(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        # corner case
        if nums[0] < nums[-1]:
            return nums[0]
        
        left = 0
        right =len(nums) - 1
        while left + 1 < right:
            mid = (left + right) // 2
            # mid in the rotated part
            # min in the left of mid
            if nums[mid] < nums[0]:
                right = mid
            else:
                left = mid + 1
        if nums[left] < nums[right]:
            return nums[left]
        return nums[right]
