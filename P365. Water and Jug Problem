# time O(log(min(x, y))
# space O(1)

class Solution(object):
    def canMeasureWater(self, x, y, z):
        
        if x + y < z:
            return False
        if x == 0 or y == 0:
            return z == 0 or x + y == z
        return z % self.gcd(x, y) == 0

    def gcd(self, x, y):
        if y == 0:
            return x
        return self.gcd(y, x % y)
