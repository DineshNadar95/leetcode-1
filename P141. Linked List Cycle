# o(n) time, O(n) space

class Solution(object):
    def hasCycle(self, head):
        
        # dic key:node
        hashset = set()
        while head:
            if head in hashset:
                return True
            else:
                hashset.add(head)
                head = head.next
        return False
            
        
# O(n) time
# O(1) space 
class Solution(object):
    def hasCycle(self, head):
        
        if not head:
            return False
        
        fast = head
        slow = head
        
        while fast.next and fast.next.next:
            fast = fast.next.next
            slow = slow.next
            if fast == slow:
                return True
            
        return False
            
    
            
