class Solution(object):
    def isValidSudoku(self, board):
        """
        :type board: List[List[str]]
        :rtype: bool
        """
        seen = []
        for i in range(9):
            for j in range(9):
                if board[i][j] == '.':
                    continue
                cur = board[i][j]
                if (i, cur) in seen or (cur, j) in seen or (i//3, j//3, cur) in seen:
                    return False
                seen.append((i, cur))
                seen.append((cur, j))
                seen.append((i//3, j//3, cur))
        return True
        
