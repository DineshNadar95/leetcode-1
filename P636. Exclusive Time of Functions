class Solution(object):
    def exclusiveTime(self, n, logs):
        """
        :type n: int
        :type logs: List[str]
        :rtype: List[int]
        """
        res = [0] * n
        stack = []
        pre_time = 0
        for log in logs:
            index, wtype, time = log.split(':')
            if wtype == 'start':
                if stack:
                    res[stack[-1]] += int(time) - pre_time
                stack.append(int(index))
                pre_time = int(time)
            else:
                res[stack[-1]]  += int(time) - pre_time + 1
                stack.pop()
                pre_time = int(time) + 1
        return res
            
