
# time O(n)

class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        count = 0
        cand = 0
        for num in nums:
            if count == 0:
                cand = num
                count = 1
            elif cand == num:
                count += 1
            else:
                count -= 1
        return cand
        
 
