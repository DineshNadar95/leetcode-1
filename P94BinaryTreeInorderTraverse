# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def inorderTraversal(self, root: TreeNode) -> List[int]:
        results = []
        self.traverse(root, results)
        return results
    
    def traverse(self, root, results):
        if not root:
            return 
        self.traverse(root.left, results)
        results.append(root.val)
        self.traverse(root.right, results)
        
# non-recursion
# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution(object):
    def inorderTraversal(self, root):
        """
        :type root: TreeNode
        :rtype: List[int]
        """
        res  = []
        stack = []
        cur = root
        
        while stack or cur:
            while cur:
                stack.append(cur)
                cur = cur.left
                
            node = stack.pop()
            res.append(node.val)
            if node.right:
                cur = node.right
                
        return res
