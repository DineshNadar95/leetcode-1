class Solution:
    def longestPalindrome(self, s: str) -> int:
        if not s:
            return 0
        map_string = {}
        length = 0
        
        for char in s:
            if char not in map_string:
                map_string[char] = 1
            else:
                map_string.pop(char)
                length += 2
        
        if len(map_string) > 0:
            length += 1
            
        return length
