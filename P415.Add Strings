# simulation 
# time num1和num2位数最大值，space O(1)
class Solution:
    def addStrings(self, num1: str, num2: str) -> str:
        m = len(num1)
        n = len(num2)
        
        if m < n:
            num1 = '0' * (n - m) + num1
        elif m > n:
            num2 = '0' * (m - n) + num2
            
        res = ''
        carry = 0
        for i in range(max(m, n) - 1, -1, -1):
            add = int(num1[i]) + int(num2[i]) + carry
            if add >= 10:
                carry = 1
                add -= 10
            else:
                carry = 0
            res = str(add) + res
            
        if carry:
            res = str(carry) + res
        return res
                
            
                
