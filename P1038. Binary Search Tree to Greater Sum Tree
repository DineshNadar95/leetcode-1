# recurions
# time O(n)
# space O(h)

class Solution(object):
    s = 0
    
    def bstToGst(self, root):
        
        if root.right:
            self.bstToGst(root.right)
        self.s += root.val
        root.val = self.s
        if root.left:
            self.bstToGst(root.left)
        return root
        
        
