# time O(m + n)
# space O(h1 + h2)

class Solution(object):
    def mergeTrees(self, t1, t2):
        if not t1 and not t2:
            return None
        
        if t1 and not t2:
            return t1
        if t2 and not t1:
            return t2
        
        new = TreeNode(t1.val + t2.val)
        new.left = self.mergeTrees(t1.left, t2.left)
        new.right = self.mergeTrees(t1.right, t2.right)
        return new
    
        
