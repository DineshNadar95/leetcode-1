# time O(numRows^2)
# space O(numRows^2)
# transmation formula: row[j] = triangle[i - 1][j - 1] + triangle[i - 1][j]

class Solution(object):
    def generate(self, numRows):
        
        triangle = []
        
        for i in range(numRows):
            row = [None for _ in range(i + 1)]
            row[0] = 1
            row[-1] = 1
            
            for j in range(1, i):
                row[j] = triangle[i - 1][j - 1] + triangle[i - 1][j]
            triangle.append(row)
            
        return triangle
