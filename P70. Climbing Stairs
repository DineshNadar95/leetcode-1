# DP
# dp[n] = dp[n-1] + dp[n-2]

# time O(n)
# spaec O(n)
class Solution(object):
    def climbStairs(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n == 1:
            return 1
        res = [0] * n
        res[0] = 1
        res[1] = 2
        for i in range(2, n):
            res[i] = res[i - 1] + res[i - 2]
        return res[-1]

# time O(n)
# space O(1)
is the same as fibo array
class Solution(object):
    def climbStairs(self, n):
        """
        :type n: int
        :rtype: int
        """
        if n == 1:
            return 1
        a, b = 1, 2
        for i in range(2, n):
            a, b = b, a + b
        return b
        
