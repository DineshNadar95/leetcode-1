# time O(n)
# space O(n)

class Solution(object):
    def groupAnagrams(self, strs):
       
        # key: sorted items, values:strings
        dic = collections.defaultdict(list)
        for str in strs:
            sorteditems = ''.join(sorted(str))
            dic[sorteditems].append(str)
        return dic.values()
