# time O(n)
# space O(n)

class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        dic = {}
        for item in strs:
            sortedItem = ''.join(sorted(item)) # string is hashable, list is not hashable
            dic[sortedItem] = dic.get(sortedItem, []) + [item]
        return dic.values()
