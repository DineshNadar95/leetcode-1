class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        hash_table = {}
        for i in range(len(nums)):
            second_num = target - nums[i]
            if second_num in hash_table:
                return [hash_table[second_num], i]
            else:
                hash_table[nums[i]] = i
