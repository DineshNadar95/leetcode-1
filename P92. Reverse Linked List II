# first go to m - 1 node
# convert from m to n
        1. # node, count is m -1 , cur = m + 1 node
        # count is n - 2, cur = n node
        2. # mid, pre均不变

class Solution(object):
    def reverseBetween(self, head, m, n):
        
        dummy = ListNode(0, head)
        cur = dummy
        c = 0
        while c < m - 1:
            cur = cur.next
            c += 1
            
        prev = cur
        mid = prev.next
        cur = mid.next
        c += 2
        
        while c <= n :
            nxt = cur.next
            cur.next = prev.next
            prev.next = cur
            mid.next = nxt
            cur = nxt
            c += 1
            
        return dummy.next
            
            
            
        
