# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def binaryTreePaths(self, root: TreeNode) -> List[str]:
        if not root:
            return []
        results = []
        self.dfs(root, "", results)
        return results
    
    def dfs(self, root, path, results):
        if not root.left and not root.right:
            results.append(path + str(root.val))
        if root.left:
            self.dfs(root.left, path + str(root.val) + "->", results)
        if root.right:
            self.dfs(root.right, path + str(root.val) + "->", results)
